{
  "name": "op-media-downloader",
  "version": "1.0.0",
  "description": "An application that lets you download all media files from a project",
  "main": "./out/main/index.js",
  "author": "On PageÂ®",
  "homepage": "https://app.onpage.it/#/",
  "scripts": {
    "generate-classes-to-keep": "node ./src/renderer/src/service/generate-classes-to-keep",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev --watch",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "npm run build && electron-builder --mac --config",
    "build:linux": "npm run build && electron-builder --linux --config"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^1.0.2",
    "@electron-toolkit/utils": "^1.0.2",
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/vue-fontawesome": "^3.0.2",
    "axios": "^1.2.1",
    "color": "^4.2.3",
    "dayjs": "^1.11.7",
    "electron-store": "^8.1.0",
    "electron-updater": "^5.3.0",
    "floating-vue": "^2.0.0-beta.20",
    "lodash": "^4.17.21",
    "mitt": "^3.0.0",
    "nodejs-file-downloader": "^4.10.3",
    "onpage-js": "^1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^1.2.3",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/color": "^3.0.3",
    "@types/lodash": "^4.14.191",
    "@types/node": "16.11.22",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "electron": "^21.3.0",
    "electron-builder": "^23.6.0",
    "electron-vite": "^1.0.14",
    "eslint": "^8.28.0",
    "eslint-plugin-vue": "^9.7.0",
    "less": "^4.1.3",
    "postcss": "^8.4.20",
    "prettier": "^2.7.1",
    "sass": "^1.57.1",
    "tailwindcss": "^3.2.4",
    "tsc": "^2.0.4",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "vue": "^3.2.45",
    "vue-i18n": "^9.2.2",
    "vue-tsc": "^1.0.9"
  }
}
